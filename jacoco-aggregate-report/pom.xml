<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<parent>
		<artifactId>uniprot-rest-api</artifactId>
		<groupId>org.uniprot</groupId>
		<version>1.0.7</version>
	</parent>
	<modelVersion>4.0.0</modelVersion>

	<artifactId>jacoco-aggregate-report</artifactId>
	<name>Aggregate Report</name>
	<dependencies>
		<dependency>
			<groupId>org.uniprot</groupId>
			<artifactId>common-rest</artifactId>
			<version>${uniprot-core.version}</version>
			<scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>org.uniprot</groupId>
			<artifactId>proteome-rest</artifactId>
			<version>${uniprot-core.version}</version>
			<scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>org.uniprot</groupId>
			<artifactId>support-data-rest</artifactId>
			<version>${uniprot-core.version}</version>
			<scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>org.uniprot</groupId>
			<artifactId>uniparc-rest</artifactId>
			<version>${uniprot-core.version}</version>
			<scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>org.uniprot</groupId>
			<artifactId>uniprotkb-rest</artifactId>
			<version>${uniprot-core.version}</version>
			<scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>org.uniprot</groupId>
			<artifactId>uniref-rest</artifactId>
			<version>${uniprot-core.version}</version>
			<scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>org.uniprot</groupId>
			<artifactId>unisave-rest</artifactId>
			<version>${uniprot-core.version}</version>
			<scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>org.uniprot</groupId>
			<artifactId>aa-rest</artifactId>
			<version>${uniprot-core.version}</version>
			<scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>org.uniprot</groupId>
			<artifactId>idmapping-rest</artifactId>
			<version>${uniprot-core.version}</version>
			<scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>org.uniprot</groupId>
			<artifactId>help-centre-rest</artifactId>
			<version>${uniprot-core.version}</version>
			<scope>compile</scope>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<groupId>org.jacoco</groupId>
				<artifactId>jacoco-maven-plugin</artifactId>
				<version>${jacoco.version}</version>
				<executions>
					<execution>
						<id>report-aggregate</id>
						<phase>verify</phase>
						<goals>
							<goal>report-aggregate</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
</project>